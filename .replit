modules = ["python-3.11", "python3"]
[agent]
integrations = ["python_anthropic:1.0.0", "python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales", "libsodium", "unzip"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VOID - The All-Seeing"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FEMTO - The Fallen Angel"

[[workflows.workflow]]
name = "VOID - The All-Seeing"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run void/main_analytics.py --server.port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "FEMTO - The Fallen Angel"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python ubik/start_solana_api.py"
waitForPort = 8000

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000
